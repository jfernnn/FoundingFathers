<%- include('../partials/header') %>

<form id="new-form" action="/founders" method="POST">
  <span>
    <label>Name:</label>
    <br><input type="text" name="name">
    <br><label>Home State:</label>
    <br><input type="text" name="homeState">
    <br><label>Was He a President?</label>
    <br><input type="text" name="wasPresident">
